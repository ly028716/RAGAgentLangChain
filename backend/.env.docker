# Docker Compose Environment Variables
# Copy this file to .env and update the values

# MySQL Configuration
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=ai_assistant
MYSQL_USER=ai_user
MYSQL_PASSWORD=ai_password
MYSQL_PORT=3306

# Redis Configuration
REDIS_PASSWORD=
REDIS_PORT=6379

# Backend Configuration
BACKEND_PORT=8000
METRICS_PORT=9090

# Application Settings
APP_NAME=AI智能助手系统
APP_VERSION=0.1.0
DEBUG=False
ENVIRONMENT=production

# JWT Configuration (IMPORTANT: Change in production!)
SECRET_KEY=change-this-secret-key-in-production-use-strong-random-string

# Tongyi Qianwen API (REQUIRED)
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# Optional: Override default settings
# TONGYI_MODEL_NAME=qwen-turbo
# DEFAULT_MONTHLY_QUOTA=100000
# MAX_UPLOAD_SIZE_MB=10
# CORS_ORIGINS=http://localhost:3000,http://localhost:5173
